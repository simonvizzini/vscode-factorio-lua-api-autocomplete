{
  "name": "factorio-lua-api-autocomplete",
  "displayName": "Factorio Lua API autocomplete",
  "description": "Autocompletion for the Factorio Lua API",
  "version": "0.7.0",
  "publisher": "svizzini",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonvizzini/vscode-factorio-lua-api-autocomplete.git"
  },
  "private": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "lua",
        "extensions": [
          ".lua"
        ],
        "aliases": [
          "lua"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.134",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "html-to-text": "^5.1.1",
    "mocha": "^6.1.4",
    "osmosis": "^1.1.10",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1",
    "vscode": "^1.1.34"
  }
}
